{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-var": [
      "error"
    ],
    "consistent-return": [
      "error"
    ],
    "no-param-reassign": [
      "error"
    ],
    "no-eval": [
      "error"
    ],
    "no-else-return": [
      "error"
    ],
    "camelcase": [
      "error"
    ],
    "consistent-this": [
      "error"
    ],
    "prefer-const": [
      "error"
    ],
    "prefer-rest-params": [
      "error"
    ],
    "prefer-template": [
      "error"
    ],
    "no-path-concat": [
      "error"
    ],
    "no-duplicate-imports": [
      "error"
    ]
  }
}
